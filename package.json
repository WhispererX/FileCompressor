{
  "name": "filecompressor",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.filecompressor.id",
    "productName": "File Compressor",
    "files": [
      "**/*"
    ],
